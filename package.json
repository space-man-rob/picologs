{
  "name": "picologs",
  "version": "0.13.0",
  "description": "A tool for viewing and filtering the logs of a game",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "tauri",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run",
    "test:browser": "vitest run --config vitest.browser.config.ts",
    "test:browser:ui": "vitest --config vitest.browser.config.ts --ui",
    "test:all": "npm run test:unit && npm run test:browser",
    "test:all:coverage": "npm run test:unit -- --coverage && npm run test:browser -- --coverage"
  },
  "license": "MIT",
  "dependencies": {
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/plugin-deep-link": "^2.4.3",
    "@tauri-apps/plugin-dialog": "~2.4.0",
    "@tauri-apps/plugin-fs": "~2.4.2",
    "@tauri-apps/plugin-http": "^2.5.2",
    "@tauri-apps/plugin-opener": "^2.5.0",
    "@tauri-apps/plugin-process": "~2.3.0",
    "@tauri-apps/plugin-store": "~2.4.0",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "@tauri-apps/plugin-websocket": "~2.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@lucide/svelte": "^0.544.0",
    "@playwright/test": "^1.56.0",
    "@sveltejs/adapter-static": "^3.0.9",
    "@sveltejs/kit": "^2.43.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@tauri-apps/cli": "^2.8.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "fuse.js": "^7.1.0",
    "happy-dom": "^19.0.2",
    "playwright": "^1.56.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.39.8",
    "svelte-check": "^4.3.2",
    "svelte-lazy": "^1.2.11",
    "tailwindcss": "^4.1.14",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "vitest-browser-svelte": "^1.1.0"
  }
}
